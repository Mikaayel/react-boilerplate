{
	"name": "react-boilerplate",
	"version": "3.0.0",
	"main": "app.js",
	"author": "mika rehman",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf ./dist/*",
		"deploy": "npm run lint && npm run prod:build && firebase deploy",
		"test": "export NODE_ENV=development && karma start",
		"lint": "eslint --fix ./src/**/*.js",
		"dev:start": "export NODE_ENV=development && webpack-dashboard -- webpack-dev-server --config webpack.config.dev.js",
		"prod:build": "export NODE_ENV=production && npm run clean && webpack -p --config webpack.config.prod.js",
		"prod:start": "node src/server.js"
	},
	"dependencies": {
		"express": "^4.16.2",
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-router": "4",
		"react-router-dom": "^4.1.1",
		"upgrade": "^1.1.0"
	},
	"devDependencies": {
		"babel": "^6.23.0",
		"babel-core": "^6.24.0",
		"babel-eslint": "^7.2.3",
		"babel-loader": "^6.4.1",
		"babel-preset-es2015": "^6.24.0",
		"babel-preset-react": "^6.23.0",
		"babel-preset-stage-0": "^6.22.0",
		"css-loader": "0.28.7",
		"eslint": "^4.4.0",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-react": "^7.1.0",
		"extract-text-webpack-plugin": "3.0.1",
		"file-loader": "^0.11.1",
		"foundation-sites": "^6.4.4-rc1",
		"html-webpack-plugin": "^2.28.0",
		"image-webpack-loader": "^3.3.0",
		"jquery": "^3.2.1",
		"node-sass": "^4.5.2",
		"postcss-loader": "^1.3.3",
		"precss": "^1.4.0",
		"react-css-modules": "^4.7.1",
		"rimraf": "^2.6.1",
		"sass-loader": "^6.0.3",
		"script-loader": "^0.7.0",
		"style-loader": "^0.16.1",
		"webpack": "^3.7.1",
		"webpack-dashboard": "^1.0.0-5",
		"webpack-dev-server": "^2.4.2"
	}
}
