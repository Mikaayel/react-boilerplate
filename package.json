{
	"name": "react-boilerplate",
	"version": "3.0.0",
	"main": "index.js",
	"author": "mika rehman",
	"license": "MIT",
	"engines": {
		"node": "8.7.0"
	},
	"scripts": {
		"clean": "rimraf ./dist/*",
		"lint": "eslint --fix ./src/**/*.js",
		"build": "npm run clean && webpack --config webpack.config.server.js && webpack --config webpack.config.client.js",
		"start": "node dist/server.js",
		"dev:server": "webpack --config webpack.config.server.js --watch",
		"dev:client": "webpack --config webpack.config.client.js --watch",
		"dev:start": "nodemon dist/server.js --watch dist/server.js"
	},
	"dependencies": {
		"express": "4.16.2",
		"node-fetch": "^1.6.3",
		"react": "16.0.0",
		"react-dom": "16.0.0",
		"react-router": "4.2.0",
		"react-router-dom": "4.2.2",
		"source-map-support": "0.5.0",
		"whatwg-fetch": "^2.0.2"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-loader": "7.1.2",
		"babel-preset-latest": "^6.22.0",
		"babel-preset-react": "^6.22.0",
		"css-loader": "0.28.7",
		"eslint": "^4.4.0",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-react": "^7.1.0",
		"extract-text-webpack-plugin": "3.0.1",
		"node-sass": "^4.5.2",
		"nodemon": "^1.11.0",
		"postcss-loader": "^1.3.3",
		"precss": "^1.4.0",
		"react-css-modules": "^4.7.1",
		"rimraf": "^2.6.1",
		"sass-loader": "^6.0.3",
		"script-loader": "^0.7.0",
		"style-loader": "^0.16.1",
		"webpack": "^3.7.1",
		"webpack-dashboard": "^1.0.0-5",
		"webpack-dev-server": "^2.4.2",
		"webpack-node-externals": "^1.6.0"
	}
}
